<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../../layout/head') %>
</head>

<body>
  <header>
    <%- include('../../layout/header') %>
  </header>
  <main>
    <% searchResults.forEach( book => { %>
    <img src=<%= book.image_url %>>
    <h3><%= book.title %></h3>
    <h5><%=book.author %></h5>
    <p><%=book.description %></p>
    <% }) %>
  </main>
</body>

</html>